{
  "inbounds": [
	{
		"type": "tun",
	    "tag": "tun-in",
	    "interface_name": "Lobsel",
		"address": [
			"172.19.0.1/30",
			"fdfe:dcba:9876::1/126"
		],
		"route_address": [
			"0.0.0.0/1",
			"128.0.0.0/1",
			"::/1",
			"8000::/1"
		],
		"route_exclude_address": [
			"192.168.0.0/16",
			"fc00::/7"
		],
	    "gso": false,
	    "auto_route": true,
	    "strict_route": true,
        "sniff_override_destination": true,
		"stack": "system",
		"sniff": true
	},
    {
      "type": "mixed",
      "tag": "mixed-in",
      "listen": "0.0.0.0",
      "listen_port": 17687,
      "sniff": true,
      "sniff_override_destination": false,
      "tcp_fast_open": false,
	  "tcp_multi_path": false,
      "udp_fragment": false,
	  "domain_strategy": "ipv4_only"
    }
  ],
  "experimental": {
    "clash_api": {
      "external_controller": "127.0.0.1:9090",
      "external_ui": "ui",
      "secret": "",
      "external_ui_download_url": "",
      "external_ui_download_detour": "direct",
      "default_mode": "rule"
    },
  "cache_file": {
    "enabled": true,
    "store_fakeip": false
    }
  },
  "ntp": {
    "enabled": true,
    "server": "ntp.aliyun.com",
    "server_port": 123,
    "interval": "30m",
    "inet4_bind_address": "0.0.0.0",
    "inet6_bind_address": "::",
	"detour": "direct"
  },
  "dns": {
    "servers": [
      {
        "tag": "google",
        "address": "tls://8.8.8.8",
		"address_strategy": "ipv4_only",
		"strategy": "ipv4_only",
		"detour": "proxy"
      },
      {
        "tag": "local",
        "address": "https://223.6.6.6/dns-query",
		"strategy": "ipv4_only",
        "detour": "direct"
      },
      {
        "tag": "dns_block",
        "address": "rcode://refused"
      }
    ],
    "rules": [
      {
        "outbound": "any",
        "server": "local"
      },
	  {
        "process_name": [
	      "ToDesk.exe",
          "ToDesk",
		  "ToDesk_Service"
        ],
        "server": "local"
      },
	  {
        "domain_keyword": [
          "oculus",
		  "oculuscdn",
		  "oculusrift",
		  "oculusvr",
		  "powersunitedvr",
		  "meta",
		  "facebook",
		  "facebook-hardware",
		  "fbcdn",
		  "gstatic",
		  "generate_204",
		  "fb",
		  "google-analytics",
		  "googleapis"
        ],
		"server": "google"
	  },
      {
        "rule_set": [
          "geosite-google",
		  "geosite-facebook"
        ],
        "server": "google"
      },
      {
        "clash_mode": "Direct",
        "server": "local"
      },
      {
        "clash_mode": "Global",
        "server": "google"
      },	
      {
        "type": "logical",
        "mode": "or",
        "rules": [
          {
            "rule_set": [
              "geosite-cn",
              "geosite-private"
            ]
          },
          {
            "domain_suffix": [
              ".cn"
            ]
          }
        ],
        "server": "local"
      },
      {
        "rule_set": [
          "geosite-category-ads-all"
        ],
        "server": "block"
      }
    ],
	"final": "google",
    "disable_cache": false,
    "disable_expire": false
  },
  "route": {
    "auto_detect_interface": true,
    "rules": [
      {
        "type": "logical",
        "mode": "or",
        "rules": [
          {
            "protocol": "dns"
          },
          {
            "port": 53
          }
        ],
        "outbound": "dns-out"
      },
	  {
        "domain_keyword": [
          "oculus",
		  "oculuscdn",
		  "oculusrift",
		  "oculusvr",
		  "powersunitedvr",
		  "meta",
		  "facebook",
		  "facebook-hardware",
		  "fbcdn",
		  "gstatic",
		  "generate_204",
		  "fb",
		  "google-analytics",
		  "googleapis"
        ],
		"outbound": "proxy"
	  },
      {
        "rule_set": [
          "geoip-google",
          "geosite-google",
		  "geoip-facebook",
		  "geosite-facebook"
        ],
        "outbound": "proxy"
      },
      {
        "ip_is_private": true,
        "outbound": "direct"
      },
      {
        "process_name": [
	      "ToDesk.exe",
          "ToDesk",
		  "ToDesk_Service"
        ],
        "outbound": "direct"
      },
      {
        "clash_mode": "direct",
        "outbound": "direct"    
      },
      {
        "clash_mode": "global", 
        "outbound": "proxy"
      },
      {
        "rule_set": [
          "geoip-cn",
          "geosite-cn",
		  "geosite-private"
        ],
        "outbound": "direct"
      },
      {
        "rule_set": "geosite-category-ads-all",
        "outbound": "block"
      }
    ],
    "final": "proxy",
    "rule_set": [
      {
        "tag": "geoip-google",
        "type": "remote",
        "format": "binary",
        "url": "https://mirror.ghproxy.com/https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geoip/google.srs",
        "download_detour": "direct"
      },
      {
        "tag": "geosite-google",
        "type": "remote",
        "format": "binary",
        "url": "https://mirror.ghproxy.com/https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geosite/google.srs",
        "download_detour": "direct"
      },
      {
        "tag": "geoip-facebook",
        "type": "remote",
        "format": "binary",
        "url": "https://mirror.ghproxy.com/https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geoip/facebook.srs",
        "download_detour": "direct"
      },
      {
        "tag": "geosite-facebook",
        "type": "remote",
        "format": "binary",
        "url": "https://mirror.ghproxy.com/https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geosite/facebook.srs",
        "download_detour": "direct"
      },
      {
        "tag": "geoip-cn",
        "type": "remote",
        "format": "binary",
        "url": "https://testingcf.jsdelivr.net/gh/MetaCubeX/meta-rules-dat@sing/geo/geoip/cn.srs",
        "download_detour": "direct"
      },
      {
        "tag": "geosite-private",
        "type": "remote",
        "format": "binary",
        "url": "https://testingcf.jsdelivr.net/gh/MetaCubeX/meta-rules-dat@sing/geo/geosite/private.srs",
        "download_detour": "direct"
      },
      {
        "tag": "geosite-cn",
        "type": "remote",
        "format": "binary",
        "url": "https://testingcf.jsdelivr.net/gh/MetaCubeX/meta-rules-dat@sing/geo/geosite/cn.srs",
        "download_detour": "direct"
      },
      {
        "tag": "geosite-category-ads-all",
        "type": "remote",
        "format": "binary",
        "url": "https://testingcf.jsdelivr.net/gh/MetaCubeX/meta-rules-dat@sing/geo/geosite/category-ads-all.srs",
        "download_detour": "direct"
      }
    ]
  },
  "outbounds": [
    {
      "tag":"proxy",
      "type":"selector",
      "outbounds":[
        "auto",
        "🇭🇰",
        "🇹🇼",
        "🇸🇬",
        "🇯🇵",
